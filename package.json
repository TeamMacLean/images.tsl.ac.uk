{
  "name": "imagehog",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "build": "parcel build public/js/src/*.js --dist-dir public/js/dist/",
    "watch": "parcel watch public/js/src/*.js --dist-dir public/js/dist/",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report",
    "test:uppy": "playwright test tests/uppy.spec.js",
    "test:thinky": "playwright test tests/thinky.spec.js",
    "test:uploader": "playwright test tests/uploader-module.spec.js",
    "test:frontend": "playwright test tests/frontpage.spec.js",
    "test:help": "playwright test tests/help.spec.js",
    "test:help-integration": "playwright test tests/help-integration.spec.js",
    "test:help-popup": "playwright test tests/help-popup.spec.js",
    "test:all": "playwright test tests/*.spec.js",
    "test:uppy:headed": "playwright test tests/uppy.spec.js --headed",
    "test:thinky:headed": "playwright test tests/thinky.spec.js --headed",
    "test:uploader:headed": "playwright test tests/uploader-module.spec.js --headed",
    "test:help:headed": "playwright test tests/help.spec.js --headed",
    "test:help-integration:headed": "playwright test tests/help-integration.spec.js --headed",
    "test:help-popup:headed": "playwright test tests/help-popup.spec.js --headed",
    "test:runner": "node run-tests.js"
  },
  "dependencies": {
    "@uppy/core": "^5.1.1",
    "@uppy/dashboard": "^5.0.4",
    "@uppy/thumbnail-generator": "^5.0.2",
    "@uppy/tus": "^5.0.2",
    "axios": "^1.6.0",
    "cookie-parser": "^1.4.6",
    "debug": "^4.3.4",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "http-errors": "^2.0.0",
    "md5-file": "^5.0.0",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-ldapauth": "^3.0.1",
    "sass": "^1.70.0",
    "sass-middleware": "^0.0.3",
    "session-rethinkdb": "^2.0.1",
    "sweetalert2": "^11.10.0",
    "thinky": "^2.3.9",
    "tus-node-server": "^0.9.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "bulma": "^0.9.4",
    "clipboard": "^2.0.11",
    "font-awesome": "^4.7.0",
    "parcel": "^2.12.0"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
