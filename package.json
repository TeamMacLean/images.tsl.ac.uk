{
  "name": "imagehog",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "build": "esbuild public/js/src/main.js public/js/src/uploader.js --bundle --outdir=public/js/dist/ --format=iife --minify --target=es2020",
    "watch": "esbuild public/js/src/main.js public/js/src/uploader.js --bundle --outdir=public/js/dist/ --format=iife --watch --target=es2020",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "dependencies": {
    "axios": "^1.6.0",
    "clipboard": "^2.0.11",
    "cookie-parser": "^1.4.6",
    "debug": "^4.3.4",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "http-errors": "^2.0.0",
    "md5-file": "^5.0.0",
    "morgan": "^1.10.0",
    "p-queue": "^7.4.1",
    "passport": "^0.7.0",
    "passport-ldapauth": "^3.0.1",
    "sass": "^1.70.0",
    "sass-middleware": "^0.0.3",
    "session-rethinkdb": "^2.0.1",
    "sweetalert2": "^11.10.0",
    "thinky": "^2.3.9",
    "tus-js-client": "^4.3.1",
    "tus-node-server": "^0.9.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "bulma": "^0.9.4",
    "esbuild": "^0.27.0",
    "font-awesome": "^4.7.0"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
